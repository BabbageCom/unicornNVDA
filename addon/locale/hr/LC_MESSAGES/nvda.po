#: addon/globalPlugins/unicorn/__init__.py:57
#: addon/globalPlugins/unicorn/dialogs.py:45
#: addon/globalPlugins/unicorn/dialogs.py:57
#: addon/globalPlugins/unicorn/dialogs.py:90
#: addon/globalPlugins/unicorn/dialogs.py:106
#: addon/globalPlugins/unicorn/dialogs.py:132
msgid "Error"
msgstr "Pogreška"

#: addon/globalPlugins/unicorn/__init__.py:58
msgid "UnicornDVC isn't available on your system."
msgstr "UnicornDVC nije dostupan u vašem sustavu."

#: addon/globalPlugins/unicorn/__init__.py:95
msgid "Connect client"
msgstr "Poveži klijent"

#: addon/globalPlugins/unicorn/__init__.py:98
msgid "Disconnect client"
msgstr "Prekini vezu s klijentom"

#: addon/globalPlugins/unicorn/__init__.py:104
msgid "Connect server"
msgstr "Poveži poslužitelj"

#: addon/globalPlugins/unicorn/__init__.py:109
msgid "Disconnect server"
msgstr "Prekini vezu s poslužiteljem"

#: addon/globalPlugins/unicorn/__init__.py:117
msgid "Mute remote"
msgstr "Utišaj udaljeno računalo"

#: addon/globalPlugins/unicorn/__init__.py:117
msgid "Mute speech and sounds from the remote computer"
msgstr "Utišava govor i zvukove udaljenog računala."

#: addon/globalPlugins/unicorn/__init__.py:125
#: addon/globalPlugins/unicorn/dialogs.py:12
msgid "UnicornDVC"
msgstr "UnicornDVC"

#: addon/globalPlugins/unicorn/__init__.py:226
msgid "Mute or unmute the speech coming from the remote computer"
msgstr "Utišava ili odtišava zvuk udaljenog računala."

#: addon/globalPlugins/unicorn/__init__.py:232
msgid "Connected in client mode!"
msgstr "Povezano u klijentskom načinu rada!"

#. Translators: Presented when connection to a remote computer was interupted.
#: addon/globalPlugins/unicorn/__init__.py:237
msgid "Connection as client interrupted"
msgstr "Veza kao klijent je prekinuta"

#: addon/globalPlugins/unicorn/__init__.py:242
msgid "Connected in server mode!"
msgstr "Povezano u načinu rada poslužitelja!"

#: addon/globalPlugins/unicorn/__init__.py:259
msgid "Error Initializing"
msgstr "Pogreška pri inicijalizaciji"

#. Translators: Message shown when cannot connect to the remote computer.
#: addon/globalPlugins/unicorn/__init__.py:261
#, python-brace-format
msgid ""
"Can't initialize UnicornDVC to create a virtual channel. Please make sure "
"that you have a valid license.\n"
"Internal error: {error}"
msgstr ""
"Nije moguće inicijalizirati UnicornDVC za stvaranje virtualnog kanala. "
"Provjerite imate li valjanu licencu.\n"
"Interna pogreška: {error}"

#: addon/globalPlugins/unicorn/__init__.py:272
#: addon/globalPlugins/unicorn/__init__.py:306
msgid "Error Connecting"
msgstr "Pogreška pri povezivanju"

#. Translators: Message shown when cannot connect to the remote computer.
#: addon/globalPlugins/unicorn/__init__.py:274
msgid ""
"Unable to connect to the virtual channel. Please make sure that your client "
"is set up correctly"
msgstr ""
"Povezivanje s virtualnim kanalom nije moguće. Provjerite je li klijent "
"ispravno postavljen"

#: addon/globalPlugins/unicorn/__init__.py:284
msgid "License Warning"
msgstr "Upozorenje o licenci"

#. Translators: Message shown when running in trial mode.
#: addon/globalPlugins/unicorn/__init__.py:286
msgid ""
"UnicornDVC is running in trial mode. If this message is unexpected, please "
"check whether you have a valid license."
msgstr ""
"UnicornDVC radi u probnom načinu rada. Ako je ova poruka neočekivana, "
"provjerite imate li valjanu licencu."

#. Translators: Title of the trial expireddialog.
#: addon/globalPlugins/unicorn/__init__.py:295
msgid "Trial Expired"
msgstr "Probna verzija je istekla"

#. Translators: Message shown when trial is expired.
#: addon/globalPlugins/unicorn/__init__.py:297
msgid "Your 10 minutes trial of UnicornDVC has expired."
msgstr "Vaših 10 minuta probnog rada UnicornDVC-a je isteklo."

#. Translators: Message shown when cannot connect to the remote computer.
#: addon/globalPlugins/unicorn/__init__.py:308
msgid ""
"Unable to connect to the virtual channel. Please make sure that you are in a "
"remote session and that your client is set up correctly"
msgstr ""
"Povezivanje s virtualnim kanalom nije moguće. Provjerite jeste li u "
"udaljenoj sesiji i je li klijent ispravno postavljen"

#: addon/globalPlugins/unicorn/dialogs.py:17
msgid "Auto-connect in server mode on startup"
msgstr "Automatsko povezivanje u načinu rada poslužitelja prilikom pokretanja"

#: addon/globalPlugins/unicorn/dialogs.py:21
msgid "Auto-connect in client mode on startup"
msgstr "Automatsko povezivanje u klijentskom načinu rada prilikom pokretanja"

#: addon/globalPlugins/unicorn/dialogs.py:26
msgid "Manage Unicorn license..."
msgstr "Upravljanje licencom dodatka Unicorn..."

#: addon/globalPlugins/unicorn/dialogs.py:44
msgid ""
"The UnicornDVC client is not available on your system. Managing a license is "
"therefore not supported."
msgstr ""
"Klijent UnicornDVC nije dostupan na vašem sustavu. Upravljanje licencom "
"stoga nije podržano."

#: addon/globalPlugins/unicorn/dialogs.py:56
msgid ""
"The UnicornDVC client available on your system is out of date. Managing a "
"license is therefore not supported."
msgstr ""
"UnicornDVC klijent dostupan na vašem sustavu je zastario. Upravljanje "
"licencom stoga nije podržano."

#: addon/globalPlugins/unicorn/dialogs.py:61
msgid "Manage Unicorn License"
msgstr "Upravljanje licencom dodatka Unicorn..."

#: addon/globalPlugins/unicorn/dialogs.py:64
msgid ""
"Your copy of UnicornDVC is properly licensed.\n"
"Choose OK to deactivate the product, or Cancel to close this dialog."
msgstr ""
"Vaš primjerak UnicornDVC-a ispravno je licenciran.\n"
"Odaberite U redu da biste deaktivirali proizvod ili Odustani da biste "
"zatvorili ovaj dijaloški okvir."

#: addon/globalPlugins/unicorn/dialogs.py:66
msgid ""
"Your copy of UnicornDVC doesn't seem to be licensed.\n"
"Enter your license key in the respective field and choose OK to activate the "
"product.\n"
"Alternatively, press Cancel to close this dialog."
msgstr ""
"Čini se da vaša kopija UnicornDVC-a nije licencirana.\n"
"Unesite licencni ključ u odgovarajuće polje i odaberite U redu da biste "
"aktivirali proizvod.\n"
"Umjesto toga, pritisnite Odustani da biste zatvorili ovaj dijaloški okvir."

#. Translators: The input field to enter the Unicorn license key
#: addon/globalPlugins/unicorn/dialogs.py:71
msgid "&License Key:"
msgstr "&licencni ključ:"

#: addon/globalPlugins/unicorn/dialogs.py:90
msgid "You must enter a valid license key."
msgstr "Morate unijeti valjani licencni ključ."

#: addon/globalPlugins/unicorn/dialogs.py:93
#: addon/globalPlugins/unicorn/dialogs.py:119
msgid "Performing request"
msgstr "Izvođenje zahtjeva"

#: addon/globalPlugins/unicorn/dialogs.py:93
msgid "Please wait while your license is being activated..."
msgstr "Pričekajte dok se vaša licenca aktivira..."

#: addon/globalPlugins/unicorn/dialogs.py:99
msgid "There was an error while performing your request."
msgstr "Došlo je do pogreške tijekom izvršavanja vašeg zahtjeva."

#: addon/globalPlugins/unicorn/dialogs.py:105
#: addon/globalPlugins/unicorn/dialogs.py:131
#, python-brace-format
msgid ""
"An error has occured:\n"
"{error}"
msgstr ""
"Došlo je do pogreške:\n"
"{error}"

#: addon/globalPlugins/unicorn/dialogs.py:113
#, python-brace-format
msgid ""
"UnicornDVC has been activated!\n"
"Additional info: {message}"
msgstr ""
"Unicorn DVC je aktiviran!\n"
"Dodatne informacije: {message}"

#: addon/globalPlugins/unicorn/dialogs.py:114
#: addon/globalPlugins/unicorn/dialogs.py:140
msgid "Congratulations!"
msgstr "Čestitamo!"

#: addon/globalPlugins/unicorn/dialogs.py:119
msgid "Please wait while your license is being deactivated..."
msgstr "Pričekajte dok se vaša licenca deaktivira..."

#: addon/globalPlugins/unicorn/dialogs.py:125
msgid "There was a timeout while performing your request."
msgstr ""
"Tijekom obrade vašeg zahtjeva došlo je do prekida veze usred vremenskog "
"ograničenja."

#: addon/globalPlugins/unicorn/dialogs.py:139
#, python-brace-format
msgid ""
"UnicornDVC has been deactivated!\n"
"Additional info: {message}"
msgstr ""
"UnicornDVC je deaktiviran!\n"
"Dodatne informacije: {message}"

#. Translators: the label of a message box dialog.
#: addon/installTasks.py:18
msgid ""
"You have installed an old version of NVDA Remote, which is incompatible with "
"the UnicornDVC add-on. Do you want to uninstall it? If you need the "
"functionality found in NVDA Remote, you are advised to get it from the NVDA "
"add-ons website."
msgstr ""
"Instalirali ste staru verziju NVDA Remotea, koja nije kompatibilna s "
"dodatkom UnicornDVC. Želite li ga deinstalirati? Ako vam je potrebna "
"funkcionalnost koja se nalazi u NVDA Remoteu, savjetujemo vam da je dobijete "
"s web-mjesta dodataka NVDA."

#. Translators: the title of a message box dialog.
#: addon/installTasks.py:20
msgid "Uninstall incompatible add-on"
msgstr "Deinstalacija nekompatibilnog dodatka"

#. Add-on summary, usually the user visible name of the addon.
#. Translators: Summary for this add-on to be shown on installation and add-on information.
#: buildVars.py:17
msgid "UnicornDVC integration for NVDA"
msgstr "Integracija UnicornDVC-a za NVDA"

#. Add-on description
#. Translators: Long description to be shown for this add-on on add-on information from add-ons manager
#: buildVars.py:20
msgid ""
"Allows sending speech and braille over RDP, Citrix and VMWare Horizon "
"virtual channels using Access4U UnicornDVC."
msgstr ""
"Omogućuje slanje govora i brajice putem RDP, Citrix i VMWare Horizon "
"virtualnih kanala pomoću Access4U UnicornDVC."
